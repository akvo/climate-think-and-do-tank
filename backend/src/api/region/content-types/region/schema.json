      "target": "api::region.region"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "news": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::news-item.news-item",
      "inversedBy": "regions"
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "regions"
    },
    "population": {
      "type": "string",
      "required": true
    },
    "area": {
      "type": "string",
      "required": true
    },
    "languages": {
      "type": "string",
      "required": true
    },
    "value_chains": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::value-chain.value-chain"
    },
    "about": {
      "type": "text"
    },
    "county_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "sub_locations": {
      "type": "component",
      "repeatable": true,
